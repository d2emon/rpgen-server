{
  "name": "rpgen",
  "version": "0.1.0",
  "description": "RPG generator",
  "private": true,
  "main": "src/index.js",
  "bin": {
    "app": "./src/index.js"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist",
    "debug": "node --debug-brk src/app.js",
    "dev": "supervisor src/app.js",
    "serve": "nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "homepage": "https://github.com/d2emon/rpgen-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d2emon/rpgen-server.git"
  },
  "author": "Dmitry Kutsenko <d2emonium@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/d2emon/rpgen-server/issues"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
